<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<!-- 에러시 window-preferences-maven검색-Download Artifact javadoc 체크할것 -->
<mapper namespace="com.example.demo.websocket.ChatDao">
	<select id="getRoomList">
		select * from chatroom order by chatroomId desc
	</select>
	<insert id="createRoom">
		insert into chatroom(title, userId) values(#{title}, #{userId})
	</insert>
	<select id="intoRoom">
		select * from chatroom where chatroomId
	</select>
	<delete id="deleteRoom">
		delete table chatroom where chatroomId=#{chatroomId}
	</delete>
</mapper>